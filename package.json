{
  "name": "@medicaboo/react-pdf-table",
  "version": "0.4.1",
  "description": "Provides tables for the @react-pdf/renderer.",
  "main": "./lib/index.js",
  "scripts": {
    "generate-barrels": "barrelsby -d src --delete -e TableRow.tsx",
    "build": "rm -rf ./lib/* && npm run generate-barrels && tsc && npm run build-storybook && npm run build-doco",
    "recompile": "tsc",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs",
    "build-doco": "typedoc --plugin typedoc-plugin-markdown --out docs/typedoc src"
  },
  "author": "medicaboo",
  "license": "MIT",
  "peerDependencies": {
    "@react-pdf/renderer": "^1.6.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@react-pdf/renderer": "^1.6.8",
    "@storybook/addon-actions": "^5.3.7",
    "@storybook/addon-info": "^5.3.7",
    "@storybook/addon-knobs": "^5.3.7",
    "@storybook/addon-links": "^5.3.7",
    "@storybook/addon-options": "^5.3.7",
    "@storybook/addon-storysource": "^5.3.7",
    "@storybook/addons": "^5.3.7",
    "@storybook/cli": "^5.3.7",
    "@storybook/react": "^5.3.7",
    "@types/enzyme": "^3.1.10",
    "@types/faker": "^4.1.5",
    "@types/node": "^13.1.8",
    "@types/sinon": "^4.3.3",
    "@types/storybook__react": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "barrelsby": "^2.1.1",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "faker": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.1",
    "mini-css-extract-plugin": "^0.7.0",
    "react": "^16.8.6",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-dom": "^16.8.6",
    "regenerator-runtime": "^0.13.2",
    "sinon": "^7.3.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.3",
    "typedoc": "^0.20.20",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^3.7.5",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  },
  "resolutions": {
    "dfa": "1.1.0"
  },
  "files": [
    "lib",
    "LICENSE",
    "main.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medicaboo/react-pdf-table.git"
  },
  "bugs": {
    "url": "https://github.com/medicaboo/react-pdf-table/issues"
  },
  "homepage": "https://github.com/medicaboo/react-pdf-table.git#README",
  "types": "./lib/index.d.ts",
  "dependencies": {}
}
